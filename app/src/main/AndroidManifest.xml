<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.netology.diplomandroidcherepanov">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.PinActivity"><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity
            android:name=".Activity.SettingsActivityPin"
            android:label="@string/tSettingsPinTitle" />
        <activity
            android:name=".Activity.NewNoteActivity"
            android:label="@string/tNewNoteTitle" />
        <activity
            android:name=".Activity.NotesListActivity"
            android:label="@string/tNotesListTitle">
        </activity>
    </application>

</manifest>
